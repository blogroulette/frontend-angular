{
  "name": "blogroulette-web-ui",
  "version": "1.0.0",
  "private": true,
  "licence": "MIT",
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "build": "npm run env -s && ng build --prod",
    "start": "npm run env -s && ng serve --aot --proxy-config proxy.conf.js",
    "serve:sw": "npm run build -s && npx http-server ./www -p 4200",
    "electron": "electron .",
    "electron:build": "ng build --prod --base-href ./ && electron .",
    "electron-package:linux64": "ng build --prod --base-href ./ && electron-packager . app --platform linux --arch x64 --out dest/ --icon resources/icon.png --overwrite",
    "preelectron-install:deb64": "ng build --prod --base-href ./ && electron-packager . app --platform linux --arch x64 --out dest/ --icon resources/icon.png --overwrite",
    "preelectron-install:rpm64": "ng build --prod --base-href ./ && electron-packager . app --platform linux --arch x64 --out dest/ --icon resources/icon.png --overwrite",
    "electron-install:deb64": "electron-installer-debian --src dest/app-linux-x64/ --dest dest/installers/ --arch amd64 --config build-deb.json",
    "electron-install:rpm64": "electron-installer-redhat --src dest/app-linux-x64/ --dest dest/installers/ --arch x86_64 --config build-rpm.json",
    "lint": "ng lint --type-check && stylelint \"src/**/*.scss\" --syntax scss && htmlhint \"src\" --config .htmlhintrc",
    "test": "ng test",
    "test:ci": "npm run lint -s && ng test --single-run --code-coverage",
    "e2e": "npm run env -s && ng e2e",
    "translations:extract": "ngx-translate-extract --input ./src --output ./src/translations/template.json --format=json --clean -sort --marker extract",
    "docs": "hads ./docs -o",
    "env": "ngx-scripts env npm_package_version",
    "cordova:prepare": "ngx-scripts cordova prepare",
    "cordova:run": "ngx-scripts cordova run",
    "cordova:build": "ngx-scripts cordova build --device --release --copy dist",
    "cordova:clean": "ngx-scripts clean",
    "cordova": "cordova",
    "generate": "ng generate"
  },
  "dependencies": {
    "@angular/animations": "^5.2.5",
    "@angular/common": "^5.2.5",
    "@angular/compiler": "^5.2.5",
    "@angular/core": "^5.2.5",
    "@angular/forms": "^5.2.5",
    "@angular/http": "^5.2.5",
    "@angular/platform-browser": "^5.2.5",
    "@angular/platform-browser-dynamic": "^5.2.5",
    "@angular/router": "^5.2.5",
    "@angular/service-worker": "^5.2.5",
    "@fortawesome/fontawesome-free-webfonts": "^1.0.4",
    "@ionic-native/core": "^4.5.3",
    "@ionic-native/keyboard": "^4.5.3",
    "@ionic-native/splash-screen": "^4.5.3",
    "@ionic-native/status-bar": "^4.5.3",
    "@ng-bootstrap/ng-bootstrap": "^1.0.0",
    "@ngx-translate/core": "^9.0.1",
    "@types/owasp-password-strength-test": "^1.3.0",
    "bootstrap": "4.0.0",
    "cordova-android": "^7.0.0",
    "cordova-custom-config": "^5.0.2",
    "cordova-ios": "^4.5.4",
    "cordova-plugin-device": "^2.0.1",
    "cordova-plugin-ionic-webview": "^1.1.16",
    "cordova-plugin-splashscreen": "^5.0.2",
    "cordova-plugin-statusbar": "^2.4.1",
    "cordova-plugin-whitelist": "^1.3.3",
    "core-js": "^2.5.2",
    "lodash": "^4.17.4",
    "ngx-markdown": "^1.5.2",
    "owasp-password-strength-test": "^1.3.0",
    "prismjs": "^1.13.0",
    "rxjs": "^5.5.5",
    "zone.js": "^0.8.18"
  },
  "devDependencies": {
    "@angular/cli": "~1.7.2",
    "@angular/compiler-cli": "^5.2.5",
    "@angular/language-service": "^5.2.5",
    "@biesbjerg/ngx-translate-extract": "^2.3.1",
    "@ngx-rocket/scripts": "^2.1.0",
    "@types/jasmine": "^2.5.52",
    "@types/jasminewd2": "^2.0.2",
    "@types/lodash": "^4.14.103",
    "@types/node": "^8.9.4",
    "codelyzer": "^4.1.0",
    "cordova": "^8.0.0",
    "electron": "^1.8.4",
    "electron-installer-debian": "^0.8.1",
    "electron-installer-redhat": "^0.5.0",
    "electron-packager": "^11.2.0",
    "hads": "^1.3.4",
    "htmlhint": "^0.9.13",
    "https-proxy-agent": "^2.0.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "^4.1.0",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "^1.1.0",
    "karma-junit-reporter": "^1.2.0",
    "protractor": "~5.3.0",
    "puppeteer": "^1.0.0",
    "stylelint": "~9.1.1",
    "stylelint-config-recommended-scss": "~3.1.0",
    "stylelint-config-standard": "~18.2.0",
    "stylelint-scss": "~2.5.0",
    "ts-node": "~5.0.0",
    "tslint": "~5.9.1",
    "typescript": "~2.6.2"
  },
  "cordova": {
    "platforms": [
      "android",
      "ios"
    ],
    "plugins": {
      "cordova-custom-config": {},
      "cordova-plugin-device": {},
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-statusbar": {},
      "cordova-plugin-whitelist": {},
      "ionic-plugin-keyboard": {},
      "cordova-plugin-ionic-webview": {}
    }
  }
}
